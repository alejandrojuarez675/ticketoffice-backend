            }
          }
        },
        "responses": {
          "200": {
            "description": "OK. Message: User created successfully and a confirmation email has been sent.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "msg": {
                      "type": "string"
                    }
                  }
                }
