  "components": {
    "securitySchemes": {
      "Authorization": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    },
    "schemas": {
      "ContactUsRequest": {
        "type": "object",
        "required": ["name", "email", "message", "subject"],
        "properties": {
          "name": {
            "type": "string",
            "description": "Full name of the person contacting"
          },
          "email": {
            "type": "string",
            "format": "email",
            "description": "Email address for response"
          },
          "message": {
            "type": "string",
            "description": "The main content of the contact message"
          },
          "subject": {
            "type": "string",
            "description": "Subject of the contact message"
          }
        }
      },
      "UserSignupRequest": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string"
          },
          "password": {
            "type": "string"
          },
          "email": {
            "type": "string"
          }
        },
        "required": [
          "username",
          "password",
          "email"
        ]
      },
      "UserLoginRequest": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        },
        "required": [
          "username",
          "password"
        ]
      },
      "LoginResponse": {
        "type": "object",
        "properties": {
          "token": {
            "type": "string"
          },
          "expiresIn": {
            "type": "integer",
            "format": "int64"
          }
        },
        "required": [
          "token",
          "expiresIn"
        ]
      },
      "CongratsCheckout": {
        "type": "object",
        "properties": {
          "qrCodes": {
            "type": "array",
            "items": {
              "type": "string",
              "format": "uri",
              "description": "URL to the QR code image",
              "example": "https://www.tuentradaya.com/admin/events/cd85b222-2adf-414d-aa26-6a0fb7c87beb/validate?sale-id=327a6f02-ac6f-49ed-b403-5ccdf9600d88"
            }
          }
        }
      },
      "CreateSessionRequest": {
        "type": "object",
        "properties": {
          "eventId": {
            "type": "string"
          },
          "priceId": {
            "type": "string"
          },
          "quantity": {
            "type": "integer"
          }
        },
        "required": [
          "eventId",
          "priceId",
          "quantity"
        ]
      },
      "SessionCreatedResponse": {
        "type": "object",
        "properties": {
          "sessionId": {
            "type": "string"
          },
          "expiredIn": {
            "type": "integer",
            "format": "int64"
          }
        },
        "required": [
          "sessionId",
          "expiredIn"
        ]
      },
      "BuyTicketsRequest": {
        "type": "object",
        "properties": {
          "mainEmail": {
            "type": "string"
          },
          "buyer": {
