  "paths": {
    "/api/public/v1/form/contact-us": {
      "post": {
        "tags": ["Forms"],
        "summary": "Submit a contact form",
        "description": "Allows users to submit a contact form with their message, which will be sent to the administrators.",
        "operationId": "submitContactForm",
        "requestBody": {
          "description": "Contact form data",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ContactUsRequest"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Contact form submitted successfully"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/ping": {
      "get": {
        "tags": [
          "System"
        ],
        "summary": "Ping",
        "operationId": "ping",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string"
                },
                "example": "pong"
              }
            }
          }
        }
      }
    },
    "/health": {
      "get": {
        "tags": [
          "System"
        ],
        "summary": "Health check",
